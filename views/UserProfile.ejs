            }
            var map = null;
            var userLatLng=null
            function fetchAdressesforMap() {
                fetch(`http://localhost:1337/userAdresses/${id}`, {
                    method: "GET"
                }).then((response) => {
                    if (response.ok) {
                        return response.json()
                    }
                }).then((data) => {
                    if ("geolocation" in navigator) {
                        // Get the user's current position
                        navigator.geolocation.getCurrentPosition(function (position) {
                             userLatLng = L.latLng(position.coords.latitude, position.coords.longitude);
                            console.log("the user lat and lng : "+userLatLng.lat,userLatLng.lng)
                            if (!map) {
                        console.log("im inside the !map here is the variable "+userLatLng)
                        map = L.map('map').setView([userLatLng.lat, userLatLng.lng], 13);
                        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
                            attribution: '© OpenStreetMap contributors'
                        }).addTo(map);
                    }
                    map.eachLayer(function (layer) {
                        if (layer instanceof L.Marker) {
                            map.removeLayer(layer);
                        }
                    });
                    var initialMarker=L.marker(userLatLng).addTo(map)
                    initialMarker.bindPopup("Your Current Position").openPopup()


                    data.adresses.forEach((adress) => {

                        var loc = L.latLng(adress.lon.value, adress.lat.value);
                        var marker = L.marker(loc).addTo(map)
                        var popupContent = `
               <div>
                <h1 style="font-weight: bold; font-family: cursive; align-self: center;"> Adresse Details </h1>
            <p> <strong> Street:</strong> ${adress.Street.value}</p>
            <p><strong> City:</strong> ${adress.City.value}</p>
            <p><strong> State:</strong> ${adress.State.value}</p>
            <p><strong> Country:</strong> ${adress.Country.value}</p>
            <button onclick="printHelloFunction('${adress.AdresseId.value}')" style="color: red;border-radius: 5px; display: flex; justify-content: center;">Delete</button>
              </div>
               `
                        marker.bindPopup(popupContent)
                        marker.on("click", function () {
                            this.openPopup()
                        })
                    })



                    map.on('click', function (e) {

                        var clickedLatLng = e.latlng;
                        console.log("here is the adresse " + clickedLatLng)


                        fetch(`https://nominatim.openstreetmap.org/reverse?format=jsonv2&lat=${clickedLatLng.lat}&lon=${clickedLatLng.lng}`)
                            .then(response => response.json())
                            .then(data => {
                                var addressDetails = data.address;
                                // var addressString =
                                //     `Street: ${addressDetails.road || 'N/A'}<br>` +
                                //     `City: ${addressDetails.city || addressDetails.town || 'N/A'}<br>` +
                                //     `State: ${addressDetails.state || 'N/A'}<br>` +
                                //     `Country: ${addressDetails.country || 'N/A'}`;

                                document.getElementById('Street').value = addressDetails.road
                                document.getElementById('City').value = addressDetails.city
                                document.getElementById('State').value = addressDetails.state
                                document.getElementById('Country').value = addressDetails.country
                                document.getElementById('lat').value = clickedLatLng.lat
                                document.getElementById('lon').value = clickedLatLng.lng
                                var marker = L.marker(clickedLatLng, {
                                    draggable: true
                                }).addTo(map);
                                marker.on("dragend", function (event) {
                                    var newvalues = marker.getLatLng()
                                    console.log("the new after drag : " + newvalues)
                                    fetch(`https://nominatim.openstreetmap.org/reverse?format=jsonv2&lat=${newvalues.lat}&lon=${newvalues.lng}`)
                                        .then(response => response.json())
                                        .then(data => {
                                            var addressDetails2 = data.address;
                                            // var addressString2 =
                                                // `Street: ${addressDetails.road || 'N/A'}<br>` +
                                                // `City: ${addressDetails.city || addressDetails.town || 'N/A'}<br>` +
                                                // `State: ${addressDetails.state || 'N/A'}<br>` +
                                                // `Country: ${addressDetails.country || 'N/A'}`;

                                            document.getElementById('Street').value = addressDetails2.road
                                            document.getElementById('City').value = addressDetails2.city
                                            document.getElementById('State').value = addressDetails2.state
                                            document.getElementById('Country').value = addressDetails2.country
                                            document.getElementById('lat').value = newvalues.lat
                                            document.getElementById('lon').value = newvalues.lng
                                             


                                        })
                                        .catch(error => {
                                            console.error('Error fetching address details:', error);
                                        });

                                })

                            })
                            .catch(error => {
                                console.error('Error fetching address details:', error);
                            });
                    });

                        });
                    } else {
                        console.log("Geolocation is not available in this browser.");
                    }
                    
                });
            }


            function printHelloFunction(id) {
                var confirmation = confirm("Are you sure you want to delete this adresse ?")
                if (confirmation) {
                    fetch(`http://localhost:1337/deleteAdresse/${id}`, {
                        method: 'POST'
                    }).then((response) => {
                        if (response.ok) {
                            return response.json()
